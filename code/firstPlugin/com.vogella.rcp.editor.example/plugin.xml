<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="com.vogella.rcp.editor.example"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="applicationCSS"
               value="platform:/plugin/com.vogella.rcp.editor.example/css/default.css">
         </property>
      </product>
   </extension>
   <extension
         id="com.vogella.rcp.editor.example.editor.taskeditor"
         name="SomeName"
         class="com.vogella.rcp.editor.example.editor.TaskEditor"
         point="org.eclipse.ui.editors">
      <editor
            class="com.vogella.rcp.editor.example.editor.TaskEditor"
            default="false"
            id="com.vogella.rcp.editor.example.editor1"
            name="Task Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="com.vogella.rcp.editor.example.handler.CallEditor"
            id="com.vogella.rcp.editor.example.openEditor"
            name="name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.vogella.rcp.editor.example.parts.TaskOverview"
            id="com.vogella.rcp.editor.example.taskoverview"
            name="Task View"
            restorable="true">
      </view>
   </extension>

</plugin>
